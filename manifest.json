{
  "manifest_version": 2,
  "name": "moodle-auto-login",
  "version": "2.0.0",
  "author": "Abdur Rahman (IIT Delhi)",
  "description": "A chrome extension that automatically logs in to IIT Delhi Moodle",
  "content_scripts": [{
    "js": ["moodle-auto-login.js"],
    "matches": ["https://moodle.iitd.ac.in/login/index.php"]
  },
  {
    "js": ["moodle-auto-login.js"],
    "matches": ["https://moodle.iitd.ac.in/login/index.php#myoverview_courses_view"]
  }]

}
